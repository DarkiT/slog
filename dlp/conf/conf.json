      "Description": "手机号",
      "EnName": "telephone_number",
      "CnName": "电话号码",
      "Level": "L4",
      "Detect": {
        "KReg": [],
        "KDict": [],
        "VReg": [
          "1(?:(((3[0-9])|(4[5-9])|(5[0-35-9])|(6[2,5-7])|(7[0135-8])|(8[0-9])|(9[0-35-9]))[ -]?\\d{4}[ -]?\\d{4})|((74)[ -]?[0-5]\\d{3}[ -]?\\d{4}))\\b"
        ],
        "VDict": []
      },
      "Filter": {
        "BAlgo": ["MASKED"],
        "BDict": [],
        "BReg": []
      },
      "Verify": {
        "CDict": [
